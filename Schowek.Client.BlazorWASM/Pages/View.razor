@using Schowek.Client.BlazorWASM.Interfaces @using Schowek.Shared.Domain.Models
@inject ICatalogService CatalogService

@page "/catalog"

<h3>Catalog</h3>
<h4>@catalog.CatalogName</h4>
@foreach (var item in catalog.Items){
    <p>@item.Content</p>
}

@code {
    Catalog? catalog; protected async override Task OnParametersSetAsync()
    {
        catalog = await CatalogService.GetCatalogAsync(1);
    }
} 
